{
  "presets": [
    [
      "@style-hooks/es",
      {
        "env": {
          "targets": {"node": "8"}
        }
      }
    ],
    [
      "@emotion/css-prop",
      {"sourceMap": false, "hoist": true, "autoLabel": false}
    ]
  ],
  "env": {
    "es": {
      "presets": [
        [
          "@style-hooks/es",
          {
            "env": {
              "modules": false,
              "targets": ">2% in US"
            },
            "objectAssign": false
          }
        ]
      ]
    }
  }
}